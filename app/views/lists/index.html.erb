<p style="color: green"><%= notice %></p>
<h1>EMPLOYEE RECORDS</h1> <br></br>


<table class="table">
<thead class="table-dark">
    <tr>
      <th>Employee Name</th>
       <th>Age</th>
       <th>Gender</th>
       <th>Designation</th>
       <th colspan="3"></th>
     </tr>
</thead>

<tbody>
    <% @lists.each do |list| %>
    <tr>
    <td><%= list.employee_name %>
    </td>
    <td><%= list.age %>
    </td>
    <td><%= list.gender %>
    </td>
    <td><%= list.designation %>
    </td>
    <td><%= link_to 'View',list %></td>
    <td><%= link_to 'Edit',edit_list_path(list) %></td>
    
    </tr>
  <% end %>
</body> 
</tbody>
</table>

<br>
<%= link_to "ADD NEW EMPLOYEE", new_list_path, class:"btn btn-secondary" %>

 